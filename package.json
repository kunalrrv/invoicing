{
  "name": "invoicing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:prod": "vue-cli-service serve --mode production",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "cy:run": "cypress run",
    "cy:run-chrome": "cypress run -b chrome",
    "cy:open": "cypress open",
    "e2e": "start-server-and-test serve 8080 cy:run",
    "e2e:chrome": "start-server-and-test serve 8080 cy:run-chrome",
    "e2e:persist": "start-server-and-test serve 8080 cy:open"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "core-js": "^3.3.2",
    "jquery": "^3.4.1",
    "lodash-es": "^4.17.21",
    "popper.js": "^1.16.0",
    "vue": "^2.6.10",
    "vue-cli-plugin-webpack-bundle-analyzer": "^2.0.0",
    "vue-router": "^3.1.3",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.7.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.5",
    "@vue/cli-plugin-vuex": "^4.0.5",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "cypress": "^3.6.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "serialize-javascript": "^2.1.1",
    "start-server-and-test": "^1.10.6",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-unused-vars": 1,
      "no-debugger": 1,
      "no-console": 1,
      "vue/no-unused-components": 1
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
